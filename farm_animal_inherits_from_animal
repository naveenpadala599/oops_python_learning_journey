class animal:
    def __init__(self, name, species):
        self.name=name
        self.species=species
    def make_sound(self):
        print(f"{self.name} makes a sound")
lion=animal("simba","lion")  
print(lion.name)
print(lion.species)
lion.make_sound()
class farm_animal(animal):
    pass
cow=farm_animal("ganga","cow")
print(cow.name)
print(cow.species)
cow.make_sound()
