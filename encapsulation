class animal:
    
    def __init__(self, name, species):
        self.name = name
        self.species = species
        self.__age = 0  # private attribute
        
    def make_sound(self):
        print(f"{self.name} makes a sound.")
        
    def get_age(self):
        return self.__age
        
    def set_age(self, age):
        if age > 0:
            self.__age = age
        else:
            print("Enter a positive number")
    
class farm_animal(animal):
    pass

class dog(farm_animal):
    def make_sound(self):
        print(f"{self.name} says woof!")
    
dog1 = dog('buddy', 'dog')
dog1.set_age(5)
print(dog1.get_age())
