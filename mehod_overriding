class Vehicle:
    def __init__(self, wheels):
        self.wheels=wheels
        
    def start_engine(self):
        print("Engine started.")
    
class Car(Vehicle):
    total_cars=0
    
    def __init__(self, color, model):
        super().__init__(4)
        self.color=color
        self.model=model
        self.speed=0
        Car.total_cars+=1
        
    def start_engine(self):
        print("Car engine started.")
        
    def accelerate(self):
        self.speed += 10
        print(f"{self.model} accelerated to {self.speed} km/h.")

    def brake(self):
        self.speed = max(0, self.speed - 10)
        print(f"{self.model} slowed down to {self.speed} km/h.")

    def stop(self):
        self.speed = 0
        print(f"{self.model} stopped moving.")  
        
    def __str__(self):
        return(f"color:{self.color}, model:{self.model}, speed:{self.speed}km/h, wheels:{self.wheels}")
v = Vehicle(2)
v.start_engine()

car = Car("red", "Toyota Supra")
car.start_engine()  
